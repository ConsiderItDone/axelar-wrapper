{
  "name": "templates-api-assemblyscript",
  "description": "Web3API Assemblyscript Template",
  "private": true,
  "version": "0.0.1-prealpha.83",
  "scripts": {
    "codegen": "npx w3 codegen",
    "build": "npx w3 build",
    "test:env:up": "npx w3 test-env up",
    "test:env:down": "npx w3 test-env down",
    "deploy": "npx w3 deploy",
    "test": "yarn test:e2e && yarn test:env:up && yarn deploy && yarn test:recipe && yarn test:env:down",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose",
    "test:sendToken": "jest sendToken --passWithNoTests --runInBand --verbose",
    "test:getDepositAddress": "jest getDepositAddress --passWithNoTests --runInBand --verbose",
    "test:integration": "jest integration --passWithNoTests --runInBand --verbose",
    "test:recipe": "npx w3 query ./recipes/e2e.json"
  },
  "devDependencies": {
    "@axelar-network/axelar-local-dev": "^1.0.0",
    "@cidt/axelar-polywrap-js": "^0.0.3",
    "@types/bn.js": "^5.1.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.11",
    "@types/prettier": "2.6.0",
    "@web3api/cli": "0.0.1-prealpha.83",
    "@web3api/client-js": "^0.0.1-prealpha.83",
    "@web3api/http-plugin-js": "0.0.1-prealpha.83",
    "@web3api/test-env-js": "0.0.1-prealpha.83",
    "@web3api/wasm-as": "0.0.1-prealpha.83",
    "assemblyscript": "0.19.1",
    "bn.js": "^5.2.1",
    "ethers": "5.0.7",
    "jest": "27.4.2",
    "nock": "^13.2.7",
    "solc": "0.8.3",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.4.3"
  }
}
